{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "prettier": true,
    "array-type": false,
    "no-console": false,
    "no-var-requires": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": false
  },
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ]
}
